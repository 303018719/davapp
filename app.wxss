/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.content-padded {padding:0 20rpx;}
.flex {
  display: flex;
}
.content{
  width: 100%;
  height: 100%;
}
page{
  width: 100%;
  height: 100%;
}
image {max-width:100%;}
.clearfix:after {
visibility: hidden;
display: block;
font-size: 0;
content: " ";
clear: both;
height: 0;
}